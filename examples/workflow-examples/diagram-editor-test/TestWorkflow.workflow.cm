workflow:
    id: test_workflow
    name: "测试工作流程"
    metadata:
        version: "1.0.0"
        author: "CrossModel Team"
        description: "用于测试图形编辑器功能的示例工作流程"
        tags:
            - "测试"
            - "图形编辑器"
            - "验证"
    nodes:
        - begin:
            id: start_node
            name: "开始测试"
            description: "测试流程的起始节点"
            position:
                x: 100
                y: 150
            testData:
                - name: "基础测试"
                  input:
                      testMode: true
                      userId: "test_user"
                  expected: "started"
        - process:
            id: validate_input
            name: "验证输入"
            description: "验证用户输入数据的有效性"
            position:
                x: 300
                y: 150
            testData:
                - name: "有效输入测试"
                  input:
                      data: { valid: true, value: "test" }
                  expected: { status: "valid", processed: true }
                  edgeBinding: "valid_edge"
                - name: "无效输入测试"
                  input:
                      data: { valid: false, value: "" }
                  expected: { status: "invalid", error: "empty_value" }
                  edgeBinding: "invalid_edge"
        - decision:
            id: check_result
            name: "检查结果"
            description: "根据验证结果进行分支判断"
            position:
                x: 500
                y: 150
        - process:
            id: process_valid
            name: "处理有效数据"
            description: "处理验证通过的数据"
            position:
                x: 700
                y: 100
        - exception:
            id: handle_invalid
            name: "处理无效数据"
            description: "处理验证失败的情况"
            expectedValue: "validation_failed"
            errorType: "validation_error"
            errorMessage: "输入数据验证失败"
            position:
                x: 700
                y: 200
        - end:
            id: success_end
            name: "成功完成"
            description: "流程成功完成"
            expectedValue: "completed"
            position:
                x: 900
                y: 100
    edges:
        - edge:
            id: start_to_validate
            source: start_node
            target: validate_input
            label: "开始验证"
        - edge:
            id: validate_to_check
            source: validate_input
            target: check_result
            label: "验证完成"
        - edge:
            id: valid_edge
            source: check_result
            target: process_valid
            condition: "valid"
            label: "数据有效"
        - edge:
            id: invalid_edge
            source: check_result
            target: handle_invalid
            condition: "invalid"
            label: "数据无效"
        - edge:
            id: process_to_end
            source: process_valid
            target: success_end
            label: "处理完成"